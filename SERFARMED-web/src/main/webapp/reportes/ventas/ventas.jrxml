	<subDataset name="venta" uuid="7ca30c88-c2dd-4e26-bfe3-b6a020610e11">
		<parameter name="idVenta" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
     venta.`idVenta` AS venta_idVenta,
     venta.`idUsuario` AS venta_idUsuario,
     venta.`comprobante` AS venta_comprobante,
     venta.`fechaHora` AS venta_fechaHora,
     servicioventa.`idServicioVenta` AS servicioventa_idServicioVenta,
     servicioventa.`idVenta` AS servicioventa_idVenta,
     servicioventa.`idServicio` AS servicioventa_idServicio,
     servicioventa.`cantidad` AS servicioventa_cantidad,
     servicioventa.`importe` AS servicioventa_importe,
     servicio.`idServicio` AS servicio_idServicio,
     servicio.`idCategoria` AS servicio_idCategoria,
     servicio.`nombre` AS servicio_nombre,
     servicio.`descripcion` AS servicio_descripcion,
     servicioventa.`precio` AS servicioventa_precio
FROM
     `venta` venta INNER JOIN `servicioventa` servicioventa ON venta.`idVenta` = servicioventa.`idVenta`
     INNER JOIN `servicio` servicio ON servicioventa.`idServicio` = servicio.`idServicio`
WHERE
     venta.`idVenta` = $P{idVenta}]]>
		</queryString>
